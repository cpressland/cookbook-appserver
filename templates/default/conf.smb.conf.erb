[global]
   workgroup = WORKGROUP
   server string = Application Server
   security = user
   map to guest = Bad User
   log level = 2
   syslog only = yes
   dns proxy = no
   guest account = apps
   load printers = no
   printing = bsd
   printcap name = /dev/null
   disable spoolss = yes

[downloads]
   path = /downloads
   public = yes
   only guest = yes
   writable = yes
